<script  type="text/javascript">
/**
 * header 초기화
 */
var pandora = '';

function initHeader() {
    if ( !$('data_form') || !$('scrap_btn')) {
        return;
    }
    var form     = $('data_form');
    var mode     = $('mode');
    var scrapBtn = $('scrap_btn');
    var mandbBtn = $('mandb_btn');
    var printBtn = $('print_btn');
    var pdfBtn   = $('pdf_btn');
    var emailBtn = $('email_btn');
    var smsBtn   = $('sms_btn');
    var editBtn  = $('edit_btn');
    
    if ( !form.event_resume_auth && mode.value != 'print' ) {
        printBtn.show();
    }

    if ( form.edit_icon_fl && form.edit_icon_fl.value == 'y' ) {
        editBtn.show();
    }
    
    // scrap button
    if ( pandora == 'y') {
        if ( form.email_service.value == true ) {
            emailBtn.show();
        }
    } else {
        // 스크랩 버튼
        if ( form.scrap_service.value == true ) {
            scrapBtn.show();
        }
        
        if ( form.mem_gb.value == 'c' ) {
            if ( form.company_cd && form.company_cd.value == 8 ) {
                return;
            }
    
            if ( form.talent_service.value == true ) {
                if ( form.has_talent_service.value == true ) {
                    mandbBtn.show();
                } else {
                    //$('request_mandb_top').show();
                    //$('request_mandb_bottom').show();
                }
            } else {
                // sms 발송 버튼
                if ( form.sms_service.value == true ) {
                    smsBtn.show();
                }
            }
    
            var serviceText = ''; 
            if ( form.mandb_search_cnt ) {
            	serviceText += '&nbsp;▶인재 연락처보기 : ';
                serviceText += '<font color="#e10b00"><b>';
                serviceText += form.mandb_search_cnt_text.value
                serviceText += '</b></font>건';
            }
            if(serviceText){
            	//$('mandb_service_text').innerHTML = serviceText;
            	//$('mandb_service_text').show();
            }       
        }
        
        // 이메일 발송 버튼
        if ( form.email_service.value == true ) {
            emailBtn.show();
        }
    }
}

function hideDisablePrintArea() {
//    $('resume_header').select('span').invoke('hide');
    //$$('.mandb-bar').invoke('hide');
    if ( $('request_mandb_top') ) {
        $('request_mandb_top').hide();
    }
    if ( $('request_mandb_bottom') ) {
        $('request_mandb_bottom').hide();
    }
    /*
    if ( $('data_form').talent_service.value == true && $('introduce_contents') ) {
